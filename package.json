{
  "name": "@geometricpanda/storybook-addon-badges",
  "version": "2.0.3",
  "description": "A Storybook addon which allows you to add badges to your stories",
  "keywords": [
    "storybook-addons",
    "storybook",
    "addon",
    "layout",
    "organize",
    "badges"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/geometricpanda/storybook-addon-badges"
  },
  "author": "Jim Drury <jim@jimdrury.co.uk>",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./manager": {
      "require": "./dist/manager.js",
      "import": "./dist/manager.mjs",
      "types": "./dist/manager.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "yarn clean",
    "build": "tsup",
    "build:watch": "yarn build --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"yarn build:watch\" \"yarn storybook --quiet\"",
    "prerelease": "zx scripts/prepublish-checks.mjs",
    "release": "yarn build && auto shipit",
    "eject-ts": "zx scripts/eject-typescript.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/blocks": "^8.3.0",
    "@storybook/components": "^8.3.0",
    "@storybook/core-events": "^8.3.0",
    "@storybook/manager-api": "^8.3.0",
    "@storybook/preview-api": "^8.3.0",
    "@storybook/react": "^8.3.0",
    "@storybook/react-vite": "^8.3.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^8.3.0",
    "@storybook/types": "^8.3.0",
    "@types/node": "^18.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "auto": "^11.1.4",
    "boxen": "^7.1.1",
    "concurrently": "^8.2.2",
    "dedent": "^1.5.3",
    "prettier": "^3.2.5",
    "prompts": "^2.4.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "storybook": "^8.3.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "vite": "^5.2.8",
    "zx": "^7.2.3"
  },
  "peerDependencies": {
    "@storybook/blocks": "^8.3.0",
    "@storybook/components": "^8.3.0",
    "@storybook/core-events": "^8.3.0",
    "@storybook/manager-api": "^8.3.0",
    "@storybook/preview-api": "^8.3.0",
    "@storybook/theming": "^8.3.0",
    "@storybook/types": "^8.3.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Badges",
    "supportedFrameworks": [
      "react",
      "vue",
      "angular",
      "web-components",
      "ember",
      "html",
      "svelte",
      "preact",
      "react-native"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "packageManager": "yarn@4.5.0"
}
